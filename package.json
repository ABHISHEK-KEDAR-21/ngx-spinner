{
  "name": "ngx-spinner",
  "version": "2.0.0",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-spinner -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Napster2210/ngx-spinner.git"
  },
  "author": {
    "name": "Yuvraj Chauhan",
    "email": "yuvrajchauhan41@gmail.com"
  },
  "keywords": [
    "angular",
    "spinner",
    "angular4",
    "angular5",
    "loading-spinner",
    "loading",
    "loading-animations",
    "loading-indicator",
    "loading-screen",
    "loadingview",
    "loading-page",
    "ngx",
    "ng4",
    "ng5",
    "loadingscreen",
    "progress",
    "progress-loader",
    "progress-indicators",
    "progress-circle",
    "loaders"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Napster2210/ngx-spinner.git/issues"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.8.6",
    "@types/node": "~6.0.60",
    "angular-in-memory-web-api": "^0.5.4",
    "codelyzer": "^4.3.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.2",
    "protractor": "~5.3.1",
    "rollup": "^0.58.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^5.5.10",
    "systemjs": "^0.21.3",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=6.0.0"
